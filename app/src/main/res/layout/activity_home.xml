<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.charleshalim.mathbraindesign2.Home"


    android:background="#214784"

    >


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="329dp"
        android:layout_height="160dp"
        app:srcCompat="@drawable/logo"
        android:layout_marginLeft="28dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="36dp" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="107dp"
        android:layout_height="69dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.13"
        app:srcCompat="@drawable/playbutton"
        app:layout_constraintHorizontal_bias="0.501"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="98dp"
        android:layout_height="73dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/versus"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton7"
        android:layout_marginLeft="88dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="99dp"
        android:layout_height="73dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/untitled"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton7"
        app:layout_constraintLeft_toRightOf="@+id/imageButton8"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.048" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="78dp"
        android:layout_height="57dp"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/exit"
        app:layout_constraintHorizontal_bias="0.993"
        android:layout_marginTop="143dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton7"
        android:layout_marginLeft="8dp" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="138dp"
        android:layout_height="74dp"
        android:layout_marginBottom="34dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/credits"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton7"
        app:layout_constraintVertical_bias="1.0" />

</android.support.constraint.ConstraintLayout>
